html
    head
        title leads
        link(rel='stylesheet', href='./css/bootstrap.min.css', type='text/css')
        script(src="./js/bootstrap.bundle.min.js")
    body
    dev(class="container")
        <h3 class= "text-center">leads details</h1>
        table(class='table table-striped')
            thead
                tr
                    th id
                    th Name
                    th Email
                    th Company
                    th Subject
                    th Message
                    th Created
                    th Country
            each item in items
                tr
                    td=item.id
                    td=item.Name
                    td=item.Email
                    td=item.Company
                    td=item.Subject
                    td=item.Message
                    td=item.created
                    td=item.Country